<!DOCTYPE html>
<html lang="en">

<head>
    <script>
        var name_recieved = "<%=name%>";
        var room_recieved = "<%=room%>";
        var dp_recieved = "<%=dp%>";
        var gmail_recieved = "<%=gmail%>"
    </script>
    <!-- prevent user to zoom in mobile -->

    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="icon" type="image/png" href="./static/media/favicon.png" />

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Hind+Siliguri&display=swap" rel="stylesheet">

    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>Room - Haveli</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script defer src='./static/js/ncc.js'></script>
    <script defer src="https://unpkg.com/peerjs@1.3.1/dist/peerjs.min.js"></script>
    <script defer src="./static/js/client.js"></script>
    <script defer src="./static/js/jqueryEffects.js"></script>

    <link rel="stylesheet" href="./static/css/style.css">
    <link rel="stylesheet" href="./static/css/style3.css">
</head>

<body>


    <div>
        <%- include('./partials/welcome'); %>
    </div>

    <%- include('./partials/entry'); %>

        <div id="start" style="display: none;">
            <%- include('./partials/nav_chat'); %>

                <br>

                <div id="main_container" style=" padding-left: 18%;display: block;height: 67vh;overflow-y: auto;display:none;    margin-top: -20px;">
                </div>
                <div style="position:relative;" id="parent_container_m">
                    <div class="container" id="container_m">
                    </div>
                    <button style="display:none; position: absolute;bottom: 9px;right: 40%;  border-radius: 50px; border: none; padding: 12px; background-color: rgba(0, 132, 207, 0.59); cursor: pointer;font-size: 46%;" id="scrollBtn" onClick="scrollBtn()">
                        <svg xmlns="http://www.w3.org/2000/svg" width="26" height="26" fill="currentColor"
                            class="bi bi-arrow-down-circle" viewBox="0 0 16 16">
                            <path fill-rule="evenodd"
                                d="M1 8a7 7 0 1 0 14 0A7 7 0 0 0 1 8zm15 0A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM8.5 4.5a.5.5 0 0 0-1 0v5.793L5.354 8.146a.5.5 0 1 0-.708.708l3 3a.5.5 0 0 0 .708 0l3-3a.5.5 0 0 0-.708-.708L8.5 10.293V4.5z" />
                        </svg></button>
                </div>
                <div class="end">
                    <div class="send" id="send_m" style="text-align: center;">
                        <form action="#" id="send-container">
                            <input type="text" name="messageInp" id="messageInp" placeholder="Say Something :)" autocomplete="off" required>
                            <button type="submit" class="round-btn" type="submit" id="send-button">
                                <img width="22px" height="22px" src="./static/media/send123.png" />
                            </button>
                            <button type="button" class="round-btn mx-1" id="stream-btn2" onClick=voiceStream()>
                                <img id="microphone" width="22px" height="22px" value=0
                                    src="./static/media/microphone.png" />

                            </button>
                            <button type="button" class="round-btn mx-1" id="stream-btn2" onClick=click_on_meme_btn()>
                                <img id="microphone" width="22px" height="22px" value=0
                                    src="./static/media/images.png" />

                            </button>

                            <div class="dropup">
                                <button type="button" class="dropbtn" id="dropbtn">
                                    <img id="microphone" width="22px" height="22px" value=0
                                        src="./static/media/options1.png" />
                                </button>
                                <div class="dropup-content" id="dropup-content">
                                    <button type="button" id="profile" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasRight" aria-controls="offcanvasRight" href="">Guest
                                        List</button>
                                    <button type="button" onClick="toogleFullScreen()" href="">Full Screen</button>
                                    <button type="button" onClick="tooglePopUp_roomInfo()">Room Info</button>
                                    <!-- <button type="button">Go Dark</button> -->
                                    <button type="button" onClick="leaveRoom()">Leave Room</button>
                                    <button type="button" onClick="tooglePopUp_report()">Report Abuse</button>

                                </div>

                            </div>
                        </form>
                    </div>

                    <button class="hidden_btn_memes" id="hidden_btn_memes" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasExample" aria-controls="offcanvasExample">
                        Button with data-bs-target
                    </button>

                    <div class="offcanvas offcanvas-start" tabindex="-1" id="offcanvasExample" aria-labelledby="offcanvasExampleLabel" style="background-color: #242e39;">
                        <div class="offcanvas-header" style="background-color: #16616e;">
                            <input type="text" name="messageInp" id="meme_search" placeholder="ðŸ”ŽSearch memes.." autocomplete="off">
                            <button class="btn btn-sm btn-success" style="height: 37px;  margin-left: -83px;" onClick="clearMemeField()">X</button>
                            <button type="button" id="meme_close_btn" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
                        </div>
                        <div class="offcanvas-body">
                            <div class="meme_container" id="container">
                                <%- include('./partials/emojis'); %>
                            </div>
                        </div>
                    </div>
                    <%- include('./partials/footer'); %>
                </div>
                <div id="vids"></div>
        </div>
        </div>
        <div>
            <%- include('./partials/popup_report'); %>

        </div>
        <div>
            <%- include('./partials/popup_roomInfo'); %>

        </div>
        <div>
            <%- include('./partials/blockedPopup'); %>
        </div>
        <a href="/" id="go_out"></a>


        <!-- No need -->
        <img style="display:none;" src="https://c.tenor.com/hdDC18mel8oAAAAj/loading-buffer.gif" alt="">
</body>
